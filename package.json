{
  "name": "didyoumean2",
  "version": "2.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": ">=4.2"
  },
  "sideEffects": false,
  "scripts": {
    "ci": "yarn lint && yarn typecheck && yarn test",
    "dev": "tsc --watch",
    "ignore-sync": "ignore-sync",
    "lint": "eslint '**/*.ts'",
    "prepublishOnly": "tsc",
    "test": "cross-env NODE_ENV=test jest --config .jestrc.json",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "leven": "^2.0.0",
    "lodash.deburr": "^4.1.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-typescript": "7.1.0",
    "@commitlint/cli": "7.5.0",
    "@commitlint/config-conventional": "7.5.0",
    "@types/jest": "24.0.0",
    "@types/leven": "2.1.1",
    "@types/lodash.deburr": "4.1.4",
    "@types/ramda": "0.25.47",
    "cross-env": "5.2.0",
    "eslint": "5.13.0",
    "eslint-config-foray1010": "6.19.0",
    "husky": "1.3.1",
    "ignore-sync": "2.0.0",
    "jest": "24.0.0",
    "lint-staged": "8.1.3",
    "markdownlint-cli": "0.13.0",
    "prettier-eslint-cli": "4.7.1",
    "typescript": "3.3.1"
  },
