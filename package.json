    "compare",
    "comparison",
    "didyoumean",
    "diff",
    "difference",
    "distance",
    "edit",
    "find",
    "fuzzy",
    "leven",
    "levenshtein",
    "match",
    "matching",
    "similar",
    "similarity",
    "suggest",
    "suggestion",
    "string",
    "text",
    "word"
  ],
  "author": "foray1010",
  "homepage": "https://github.com/foray1010/didyoumean2",
  "repository": "foray1010/didyoumean2",
  "bugs": "https://github.com/foray1010/didyoumean2/issues"
}
