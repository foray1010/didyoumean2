{
  "$schema": "https://json.schemastore.org/package",
  "name": "didyoumean2",
  "version": "6.0.1",
  "description": "a library for matching human-quality input to a list of potential matches using the Levenshtein distance algorithm",
  "keywords": [
    "closest",
    "compare",
    "comparison",
    "didyoumean",
    "diff",
    "difference",
    "distance",
    "edit",
    "find",
    "fuzzy",
    "leven",
    "levenshtein",
    "match",
    "matching",
    "similar",
    "similarity",
    "string",
    "suggest",
    "suggestion",
    "text",
    "typescript",
    "word"
  ],
  "homepage": "https://github.com/foray1010/didyoumean2",
  "bugs": "https://github.com/foray1010/didyoumean2/issues",
  "repository": "foray1010/didyoumean2",
  "license": "MIT",
  "author": "foray1010",
  "sideEffects": false,
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": {
      "import": "./dist/index.d.mts",
      "require": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.cjs",
  "unpkg": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "*.md"
  ],
  "scripts": {
    "build": "yarn clean:build && tsc --project tsconfig.build.json && rollup --configPlugin \"babel={ babelHelpers: 'runtime', extensions: ['.js', '.ts'] }\" -c",
    "ci": "yarn build && npm-run-all -p 'lint:*' 'type:*' && yarn test && size-limit",
    "clean:build": "shx rm -rf dist/ build/dts/",
    "postinstall": "husky install",
    "lint": "npm-run-all -p 'lint:*'",
    "lint:js": "eslint .",
    "lint:md": "yarn remark .",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettier": "prettier --ignore-path=node_modules/@foray1010/prettier-config/prettierignore",
    "remark": "remark --frail --ignore-path=node_modules/@foray1010/remark-preset/remarkignore --ignore-path-resolve-from=cwd --silently-ignore",
    "test": "jest",
    "type:check": "tsc",
    "type:coverage": "type-coverage --strict --at-least 99 --detail --ignore-catch"
  },
  "dependencies": {
    "@babel/runtime": "^7.19.0",
    "fastest-levenshtein": "^1.0.12",
    "lodash.deburr": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.2",
    "@commitlint/cli": "18.1.0",
    "@commitlint/config-conventional": "18.1.0",
    "@foray1010/babel-preset": "10.0.0",
    "@foray1010/eslint-config": "11.0.3",
    "@foray1010/jest-preset": "4.0.0",
    "@foray1010/prettier-config": "10.0.0",
    "@foray1010/remark-preset": "9.0.1",
    "@foray1010/tsconfig": "11.0.2",
    "@rollup/plugin-babel": "6.0.4",
    "@rollup/plugin-commonjs": "25.0.7",
    "@rollup/plugin-node-resolve": "15.2.3",
    "@rollup/plugin-terser": "0.4.4",
    "@size-limit/preset-small-lib": "10.0.2",
    "@types/jest": "29.5.8",
    "@types/lodash.deburr": "4.1.8",
    "eslint": "8.53.0",
    "husky": "8.0.3",
    "ignore-sync": "7.0.1",
    "jest": "29.7.0",
    "lint-staged": "15.0.2",
    "node-notifier": "10.0.1",
    "npm-run-all": "4.1.5",
    "pinst": "3.0.0",
    "prettier": "3.0.3",
    "remark-cli": "12.0.0",
    "rollup": "4.3.0",
    "rollup-plugin-dts": "6.1.0",
    "shx": "0.3.4",
    "size-limit": "10.0.2",
    "type-coverage": "2.27.0",
    "typescript": "5.2.2"
  },
  "packageManager": "yarn@4.0.1",
  "engines": {
    "node": "^18.12.0 || >=20.9.0"
  }
}
