{
  "name": "didyoumean2",
  "version": "1.0.1",
  "description": "a library for matching human-quality input to a list of potential matches using the Levenshtein distance algorithm",
  "main": "index.js",
  "engines": {
    "node": ">= 4.2"
  },
  "scripts": {
    "coverage": "nyc --all --reporter=lcov npm test",
    "lint": "gulp lint",
    "test": "ava --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/foray1010/didyoumean2.git"
  },
  "keywords": [
    "closest",
    "compare",
    "comparison",
    "didyoumean",
    "diff",
    "difference",
    "distance",
    "edit",
    "find",
    "fuzzy",
    "leven",
    "levenshtein",
    "match",
    "matching",
    "similar",
    "similarity",
    "suggest",
    "suggestion",
    "string",
    "text",
    "word"
  ],
  "author": "foray1010",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/foray1010/didyoumean2/issues"
  },
  "homepage": "https://github.com/foray1010/didyoumean2#readme",
  "dependencies": {
    "leven": "^2.0.0",
    "lodash.get": "^4.0.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "eslint": "^2.3.0",
    "eslint-config-foray1010": "^1.3.0",
    "gulp": "^3.9.0",
    "gulp-eslint": "^2.0.0",
    "lodash.set": "^4.0.0",
    "seamless-immutable": "^5.0.1"
  },
  "config": {
    "nyc": {
      "exclude": [
        "coverage",
        "gulpfile.js",
        "test"
      ]
    }
  }
}
